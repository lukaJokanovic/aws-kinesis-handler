  "license": "ISC",
  "scripts": {
    "build": "tsc -p ./",
    "start:prod": "node ./build/main.js",
    "start:dev": "node --inspect=0.0.0.0:9999 ./build/main.js",
    "start:nodemon": "npx nodemon -e ts --exec 'yarn run build && yarn run start:dev'",
    "format": "prettier --write \"src/**/*.ts\"",
    "populate": "node ./build/scripts/InsertData.script.js",
    "lint:check": "eslint -c .eslintrc.js \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint -c .eslintrc.js \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "mocha 'src/test/**/*.test.ts'",
    "coverage:run": "nyc --silent yarn run test:unit",
    "coverage:report": "nyc report --reporter=lcov --reporter=text",
    "coverage": "yarn run coverage:run && yarn run coverage:report"
  },
  "dependencies": {
    "@aws-lambda-powertools/logger": "^2.0.2",
    "@aws-sdk/client-kinesis": "^3.529.1",
    "@aws-sdk/node-http-handler": "^3.374.0",
    "joi": "^17.10.2"
  },
  "devDependencies": {
    "@hapi/joi": "^17.1.1",
    "@types/chai": "^4.3.11",
    "@types/chai-as-promised": "^7.1.6",
    "@types/hapi__joi": "^17.1.11",
    "@types/joi": "^17.2.3",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.26",
    "@types/sinon": "^10.0.17",
    "@types/sinon-chai": "^3.2.10",
    "@types/sinon-express-mock": "^1.3.10",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.0.0",
    "chai": "^4.3.10",
    "chai-as-promised": "^7.1.1",
    "dotenv": "^16.0.1",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.19",
    "nyc": "^15.1.0",
    "prettier": "^2.3.2",
    "sinon": "^15.0.0",
    "sinon-chai": "^3.7.0",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.2",
    "typescript": "^4.3.5"
  }
}
